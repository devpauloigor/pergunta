<%- model_class = QuestaoSubgrupo -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:questao_id) %>:</strong></dt>
  <dd><%= @questao_subgrupo.questao_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:sub_grupo_id) %>:</strong></dt>
  <dd><%= @questao_subgrupo.sub_grupo_id %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              questao_subgrupos_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_questao_subgrupo_path(@questao_subgrupo), :class => 'btn btn-primary' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              questao_subgrupo_path(@questao_subgrupo),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>

              
</div>
