<%- model_class = QuestaoSubgrupo -%>

<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:questao_id) %></th>
      <th><%= model_class.human_attribute_name(:sub_grupo_id) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @questao_subgrupos.each do |questao_subgrupo| %>



    <tr>
      
        <td><%= questao_subgrupo.questao_id %></td>
        <td><%= questao_subgrupo.sub_grupo_id %></td>
        <td><%=l questao_subgrupo.created_at %></td>
   
  
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_questao_subgrupo_path(questao_subgrupo), :class => 'btn btn-primary btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      questao_subgrupo_path(questao_subgrupo),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-danger btn-xs' %>
        </td>
      </tr>
      <%end%>
  
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_questao_subgrupo_path,
            :class => 'btn btn-primary' %>
