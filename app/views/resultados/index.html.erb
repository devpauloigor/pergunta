<p id="notice"><%= notice %></p>

<h1>Listing Resultados</h1>

<table>
  <thead>
    <tr>
      <th>Provaaluno</th>
      <th>Questao</th>
      <th>Resposta</th>
      <th>Correta</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @resultados.each do |resultado| %>
      <tr>
        <td><%= resultado.provaaluno %></td>
        <td><%= resultado.questao %></td>
        <td><%= resultado.resposta %></td>
        <td><%= resultado.correta %></td>
        <td><%= link_to 'Show', resultado %></td>
        <td><%= link_to 'Edit', edit_resultado_path(resultado) %></td>
        <td><%= link_to 'Destroy', resultado, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Resultado', new_resultado_path %>
