<p id="notice"><%= notice %></p>

<h1>Listing Tempandos</h1>

<table>
  <thead>
    <tr>
      <th>Horario</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tempandos.each do |tempando| %>
      <tr>
        <td><%= tempando.horario %></td>
        <td><%= link_to 'Show', tempando %></td>
        <td><%= link_to 'Edit', edit_tempando_path(tempando) %></td>
        <td><%= link_to 'Destroy', tempando, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tempando', new_tempando_path %>
